{
  "name": "inline-css",
  "version": "4.0.3",
  "description": "Inline css into an html file.",
  "main": "index.js",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "cheerio": "^1.0.0",
    "css-rules": "^1.1.0",
    "extract-css": "^3.0.2",
    "flat-util": "^1.1.9",
    "pick-util": "^1.1.5",
    "slick": "^1.12.2",
    "specificity": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@changesets/cli": "^2.27.11",
    "eslint": "^8.33.0",
    "js-beautify": "^1.15.1",
    "mocha": "^11.0.1",
    "nyc": "^17.1.0",
    "should": "^13.2.3",
    "turbo": "^2.3.3",
    "vinyl": "^3.0.0"
  },
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "mocha test packages/**/test",
    "lint": "eslint index.js lib/ packages/",
